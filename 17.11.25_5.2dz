l = []
def f(num):
    rez = ""
    while num:
        rez += str(num % 3)
        num //= 3
    return rez[::-1]
for N in range(1, 10000):
    R = f(N)
    if sum(map(int, R)) % 2 == 0:
        R = "12" + R[2:] + "0"
    else:
        R = "10" + R[2:] + "2"
    if int(R, 3) > 105:
        print(N)
        break
